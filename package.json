{
    "name": "codeaccepted",
    "displayName": "CodeAccepted",
    "description": "A VS Code extension to help codeforces users upsolve and give contests conveniently and swiftly",
    "license": "GPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/baoguoma/CodeAccepted"
    },
    "publisher": "kebin-peng",
    "icon": "res/svg/logo.png",
    "version": "1.7.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "type": "commonjs",
    "contributes": {
        "commands": [
            {
                "command": "codeaccepted.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "codeaccepted.reloadContests",
                "title": "Refresh contest list",
                "icon": {
                    "dark": "res/svg/refresh.svg",
                    "light": "res/svg/refresh.svg"
                }
            },
            {
                "command": "codeaccepted.getProblemFilters",
                "title": "Filter",
                "icon": {
                    "dark": "res/svg/filter.png",
                    "light": "res/svg/filter.png"
                }
            },
            {
                "command": "codeaccepted.reloadProblems",
                "title": "Refresh problem list",
                "icon": {
                    "dark": "res/svg/refresh.svg",
                    "light": "res/svg/refresh.svg"
                }
            },
            {
                "command": "codeaccepted.copyContestURL",
                "title": "Copy Contest URL",
                "icon": {
                    "dark": "res/svg/copy.png",
                    "light": "res/svg/copy.png"
                }
            },
            {
                "command": "codeaccepted.createContestDirectory",
                "title": "Create Contest Folder",
                "icon": {
                    "dark": "res/svg/CreateFolder.png",
                    "light": "res/svg/CreateFolder.png"
                }
            },
            {
                "command": "codeaccepted.manualContestFolderCreation",
                "title": "Manually Create Contest Folder",
                "icon": {
                    "dark": "res/svg/CreateFolder.png",
                    "light": "res/svg/CreateFolder.png"
                }
            },
            {
                "command": "codeaccepted.registerContest",
                "title": "Register for Contest",
                "icon": {
                    "dark": "res/svg/registration.png",
                    "light": "res/svg/registration.png"
                }
            },
            {
                "command": "codeaccepted.openContest",
                "title": "Open Contest Page",
                "icon": {
                    "dark": "res/svg/question_mark.png",
                    "light": "res/svg/question_mark.png"
                }
            },
            {
                "command": "codeaccepted.copyProblemURL",
                "title": "Copy Problem URL",
                "icon": {
                    "dark": "res/svg/copy.png",
                    "light": "res/svg/copy.png"
                }
            },
            {
                "command": "codeaccepted.createProblemDirectory",
                "title": "Create Problem Folder",
                "icon": {
                    "dark": "res/svg/CreateFolder.png",
                    "light": "res/svg/CreateFolder.png"
                }
            },
            {
                "command": "codeaccepted.manualProblemFolderCreation",
                "title": "Manually Create Problem Folder",
                "icon": {
                    "dark": "res/svg/CreateFolder.png",
                    "light": "res/svg/CreateFolder.png"
                }
            },
            {
                "command": "codeaccepted.createContestProblemDirectory",
                "title": "Create only this problem Folder",
                "icon": {
                    "dark": "res/svg/CreateFolder.png",
                    "light": "res/svg/CreateFolder.png"
                }
            },
            {
                "command": "codeaccepted.runTestCases",
                "title": "Run Test Cases",
                "icon": {
                    "dark": "res/svg/play_button.png",
                    "light": "res/svg/play_button.png"
                }
            },
            {
                "command": "codeaccepted.addTestCases",
                "title": "Add Test Cases",
                "icon": {
                    "dark": "res/svg/add_TestCase.png",
                    "light": "res/svg/add_TestCase.png"
                }
            },
            {
                "command": "codeaccepted.openProblemStatement",
                "title": "Open Problem Statement",
                "icon": {
                    "dark": "res/svg/question_mark.png",
                    "light": "res/svg/question_mark.png"
                }
            },
            {
                "command": "codeaccepted.openProblemURL",
                "title": "Open Problem Statement URL",
                "icon": {
                    "dark": "res/svg/question_mark.png",
                    "light": "res/svg/question_mark.png"
                }
            },
            {
                "command": "codeaccepted.copyContestProblemURL",
                "title": "Copy Problem URL",
                "icon": {
                    "dark": "res/svg/copy.png",
                    "light": "res/svg/copy.png"
                }
            },
            {
                "command": "codeaccepted.openContestProblem",
                "title": "Open Contest Problem Statement",
                "icon": {
                    "dark": "res/svg/question_mark.png",
                    "light": "res/svg/question_mark.png"
                }
            },
            {
                "command": "codeaccepted.submitProblem",
                "title": "Submit Problem",
                "icon": {
                    "dark": "res/svg/submit.svg",
                    "light": "res/svg/submit.svg"
                }
            },
            {
                "command": "codeaccepted.createStressTestingFiles",
                "title": "Create Stress Testing Files",
                "icon": {
                    "dark": "res/svg/createStressTestingFiles.png",
                    "light": "res/svg/createStressTestingFiles.png"
                }
            },
            {
                "command": "codeaccepted.createStressTestingFiles",
                "title": "Create Stress Testing Files",
                "icon": {
                    "dark": "res/svg/createStressTestingFiles.png",
                    "light": "res/svg/createStressTestingFiles.png"
                }
            },
            {
                "command": "codeaccepted.stressTest",
                "title": "Stress Test",
                "icon": {
                    "dark": "res/svg/stressTest.png",
                    "light": "res/svg/stressTest.png"
                }
            },
            {
                "command": "codeaccepted.createAclCombinedFile",
                "title": "Create ACL combined file",
                "icon": {
                    "dark": "res/svg/createAclCombinedFile.png",
                    "light": "res/svg/createAclCombinedFile.png"
                }
            },
            {
                "command": "codeaccepted.openAclDocumentation",
                "title": "Open ACL Documentation",
                "icon": {
                    "dark": "res/svg/openAclDocumentation.png",
                    "light": "res/svg/openAclDocumentation.png"
                }
            },
            {
                "command": "codeaccepted.stopStressTesting",
                "title": "Stop Stress Testing"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "codeaccepted",
                    "title": "CodeAccepted",
                    "icon": "res/svg/graph.svg"
                }
            ]
        },
        "views": {
            "codeaccepted": [
                {
                    "id": "codeacceptedContests",
                    "name": "Contests",
                    "icon": "res/svg/icon.svg",
                    "contextualTitle": "Contests",
                    "visibility": "collapsed"
                },
                {
                    "id": "codeacceptedProblems",
                    "name": "Problems",
                    "icon": "res/svg/icon.svg",
                    "contextualTitle": "Problems",
                    "visibility": "collapsed"
                },
                {
                    "id": "codeacceptedProfile",
                    "name": "User Profile",
                    "icon": "res/svg/icon.svg",
                    "contextualTitle": "Profile",
                    "visibility": "collapsed"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "codeaccepted.reloadContests",
                    "when": "view == codeacceptedContests",
                    "group": "navigation"
                },
                {
                    "command": "codeaccepted.manualContestFolderCreation",
                    "when": "view == codeacceptedContests",
                    "group": "navigation"
                },
                {
                    "command": "codeaccepted.getProblemFilters",
                    "when": "view == codeacceptedProblems",
                    "group": "navigation"
                },
                {
                    "command": "codeaccepted.reloadProblems",
                    "when": "view == codeacceptedProblems",
                    "group": "navigation"
                },
                {
                    "command": "codeaccepted.manualProblemFolderCreation",
                    "when": "view == codeacceptedProblems",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "codeaccepted.copyContestURL",
                    "when": "view == codeacceptedContests && viewItem==contest",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.openContest",
                    "when": "view == codeacceptedContests && viewItem==contest",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.createContestDirectory",
                    "when": "view == codeacceptedContests && viewItem==contest",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.registerContest",
                    "when": "view == codeacceptedContests && viewItem==FutureContest",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.createProblemDirectory",
                    "when": "view == codeacceptedProblems && viewItem==problem",
                    "group": "inline"
                },
                {
                    "when": "view == codeacceptedProblems && viewItem==problem",
                    "command": "codeaccepted.openProblemURL",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.copyProblemURL",
                    "when": "view == codeacceptedProblems && viewItem==problem",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.createContestProblemDirectory",
                    "when": "view == codeacceptedContests && viewItem==contestproblem",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.copyContestProblemURL",
                    "when": "view == codeacceptedContests && viewItem==contestproblem",
                    "group": "inline"
                },
                {
                    "command": "codeaccepted.openContestProblem",
                    "when": "view == codeacceptedContests && viewItem==contestproblem",
                    "group": "inline"
                }
            ],
            "editor/title": [
                {
                    "when": "resourceExtname == .hs || resourceExtname == .kt || resourceExtname == .cpp || resourceExtname == .c || resourceExtname == .py || resourceExtname == .java",
                    "command": "codeaccepted.runTestCases",
                    "group": "navigation"
                },
                {
                    "when": "resourceExtname == .hs || resourceExtname == .kt || resourceExtname == .cpp || resourceExtname == .c || resourceExtname == .py || resourceExtname == .java",
                    "command": "codeaccepted.addTestCases",
                    "group": "navigation"
                },
                {
                    "when": "resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.hs$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.kt$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.cpp$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.c$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.py$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.java$/'",
                    "command": "codeaccepted.submitProblem",
                    "group": "navigation"
                },
                {
                    "when": "resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.hs$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.kt$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.cpp$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.c$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.py$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.java$/'",
                    "command": "codeaccepted.openProblemStatement",
                    "group": "navigation"
                },
                {
                    "when": "resourceFilename != gen.hs && resourceFilename != brute.hs && resourceExtname == .hs || resourceFilename != gen.kt && resourceFilename != brute.kt && resourceExtname == .kt || resourceFilename != gen.cpp && resourceFilename != brute.cpp && resourceExtname == .cpp || resourceFilename != gen.c && resourceFilename != brute.c && resourceExtname == .c || resourceFilename != gen.py && resourceFilename != brute.py && resourceExtname == .py || resourceFilename != gen.java && resourceFilename != brute.java && resourceExtname == .java",
                    "command": "codeaccepted.stressTest",
                    "group": "navigation"
                },
                {
                    "when": "resourceFilename != gen.kt && resourceFilename != brute.kt && resourceExtname == .kt || resourceFilename != gen.cpp && resourceFilename != brute.cpp && resourceExtname == .cpp || resourceFilename != gen.c && resourceFilename != brute.c && resourceExtname == .c || resourceFilename != gen.py && resourceFilename != brute.py && resourceExtname == .py || resourceFilename != gen.java && resourceFilename != brute.java && resourceExtname == .java",
                    "command": "codeaccepted.createStressTestingFiles",
                    "group": "navigation"
                },
                {
                    "when": "resourceFilename != combined.cpp && resourceExtname == .cpp",
                    "command": "codeaccepted.createAclCombinedFile",
                    "group": "navigation"
                },
                {
                    "when": "resourceExtname == .cpp && config.codeaccepted.enableAclDocumentationButton",
                    "command": "codeaccepted.openAclDocumentation",
                    "group": "navigation"
                }
            ]
        },
        "configuration": {
            "title": "codeaccepted",
            "properties": {
                "codeaccepted.compilationLanguage": {
                    "type": "string",
                    "default": "g++",
                    "group": "language",
                    "enum": [
                        "g++",
                        "gcc",
                        "java",
                        "python",
                        "python2",
                        "python3",
                        "kotlin",
                        "haskell"
                    ],
                    "markdownDescription": "Note : Incase of python please choose the python prefix you would generally use on your system to run"
                },
                "codeaccepted.codeTemplatePath": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of the `template` that has to be used when a folder is created"
                },
                "codeaccepted.generatorTemplatePath": {
                    "type": "string",
                    "default": "",
                    "markdownDescription": "The path of the generator `template` that has to be used when stress testing is invoked"
                },
                "codeaccepted.g++ CompilationFlags": {
                    "type": "string",
                    "default": "-std=c++14",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to g++"
                },
                "codeaccepted.gccCompilationFlags": {
                    "type": "string",
                    "default": "",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to gcc"
                },
                "codeaccepted.pythonCompilationFlags": {
                    "type": "string",
                    "default": "",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to python and python3"
                },
                "codeaccepted.javaCompilationFlags": {
                    "type": "string",
                    "default": "",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to java"
                },
                "codeaccepted.kotlinCompilationFlags": {
                    "type": "string",
                    "default": "",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to kotlin"
                },
                "codeaccepted.haskellCompilationFlags": {
                    "type": "string",
                    "default": "",
                    "group": "language",
                    "markdownDescription": "Space separated additional compilation flags passed to haskell"
                },
                "codeaccepted.codeforcesHandle": {
                    "type": "string",
                    "default": "",
                    "group": "profile",
                    "markdownDescription": "Specify your handle on Codeforces to get a more personalized environment"
                },
                "codeaccepted.numberOfStressTestingTestCases": {
                    "type": "number",
                    "default": "",
                    "markdownDescription": "Enter number of testcases the stress testing generator should make. [10-10000]"
                },
                "codeaccepted.enableAclSupport": {
                    "type": "boolean",
                    "default": "",
                    "markdownDescription": "Enable Atcoder Library support"
                },
                "codeaccepted.enableAclDocumentationButton": {
                    "type": "boolean",
                    "default": "",
                    "markdownDescription": "Enable button that links to ACL documentation website"
                },
                "codeaccepted.hostname": {
                    "title": "Default Hostname",
                    "type": "string",
                    "description": "hostname for codeforces, add CodeForces mirror hostname here incase of problem with orignal",
                    "default": "codeforces.com"
                }
            }
        },
        "keybindings": [
            {
                "command": "codeaccepted.addTestCases",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "resourceExtname == .hs || resourceExtname == .kt || resourceExtname == .cpp || resourceExtname == .c || resourceExtname == .py || resourceExtname == .java"
            },
            {
                "command": "codeaccepted.runTestCases",
                "key": "ctrl+alt+r",
                "mac": "cmd+alt+r",
                "when": "resourceExtname == .hs || resourceExtname == .kt || resourceExtname == .cpp || resourceExtname == .c || resourceExtname == .py || resourceExtname == .java"
            },
            {
                "command": "codeaccepted.submitProblem",
                "key": "ctrl+alt+s",
                "mac": "cmd+alt+s",
                "when": "resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.hs$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.kt$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.cpp$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.c$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.py$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.java$/'"
            },
            {
                "command": "codeaccepted.openProblemStatement",
                "key": "ctrl+alt+o",
                "mac": "cmd+alt+o",
                "when": "resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.hs$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.kt$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.cpp$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.c$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.py$/' || resource =~ '/.*[A-Z][1-9]?_.*/[A-Z][1-9]?_.*\\.java$/'"
            },
            {
                "command": "codeaccepted.stopStressTesting",
                "key": "ctrl+shift+z",
                "mac": "cmd+shift+z",
                "when": "editorTextFocus"
            },
            {
                "command": "codeaccepted.stressTest",
                "key": "ctrl+alt+z",
                "mac": "cmd+alt+z",
                "when": "resourceFilename != gen.kt && resourceFilename != brute.kt && resourceExtname == .kt || resourceFilename != gen.cpp && resourceFilename != brute.cpp && resourceExtname == .cpp || resourceFilename != gen.c && resourceFilename != brute.c && resourceExtname == .c || resourceFilename != gen.py && resourceFilename != brute.py && resourceExtname == .py || resourceFilename != gen.java && resourceFilename != brute.java && resourceExtname == .java"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack --config ./build/node-extension.webpack.config.js",
        "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
        "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "yarn run test-compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "prewebpack": "webpack --mode development --config ./build/webpack.frontend.config.js"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.23",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^8.0.4",
        "@types/node": "^14.18.31",
        "@types/node-fetch": "^2.5.7",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/selenium-webdriver": "^4.0.11",
        "@types/vscode": "^1.52.0",
        "@types/websocket": "^1.0.5",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.2.3",
        "mocha": "^8.4.0",
        "node-loader": "^2.0.0",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0"
    },
    "dependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-react": "^7.18.6",
        "axios": "^0.21.1",
        "axios-cookiejar-support": "^1.0.1",
        "babel-loader": "^8.2.5",
        "bootstrap": "^5.2.1",
        "cheerio": "^1.0.0-rc.10",
        "child_process": "^1.0.2",
        "copy-webpack-plugin": "^11.0.0",
        "debug": "^4.3.4",
        "fs": "^0.0.1-security",
        "html-entities": "^2.3.3",
        "keytar": "^7.9.0",
        "node-fetch": "^2.6.1",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tough-cookie": "^4.1.2",
        "urijs": "^1.19.11",
        "websocket": "^1.0.34"
    }
}